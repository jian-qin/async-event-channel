{
  "name": "async-event-channel",
  "description": "Asynchronous event channels",
  "version": "2.1.2",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "README.zh-CN.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc & pnpm run esbuild:cjs & pnpm run esbuild:esm",
    "esbuild:cjs": "esbuild src/index.ts --bundle --platform=node --target=esnext --outfile=dist/index.cjs.js --format=cjs",
    "esbuild:esm": "esbuild src/index.ts --bundle --platform=neutral --target=esnext --outfile=dist/index.esm.js --format=esm",
    "test": "echo \"no test specified\""
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jian-qin/async-event-channel.git"
  },
  "keywords": [
    "cache emit",
    "two-way communication",
    "scope cancel",
    "task queue",
    "typescript"
  ],
  "author": "jian-qin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jian-qin/async-event-channel/issues"
  },
  "homepage": "https://github.com/jian-qin/async-event-channel#readme"
